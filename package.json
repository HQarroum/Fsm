{
  "name": "fsm.js",
  "version": "2.0.1",
  "description": "A tiny implementation of a finite state machine.",
  "main": "dist/fsm.esm.js",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/fsm.cjs",
      "import": "./dist/fsm.esm.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HQarroum/Fsm.git"
  },
  "scripts": {
    "build": "npx rollup -c",
    "test": "npx cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "npx eslint .",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "fsm",
    "state",
    "machine"
  ],
  "author": "Halim Qarroum",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HQarroum/Fsm/issues"
  },
  "devDependencies": {
    "eslint": "^8.25.0",
    "jest": "^29.1.2",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "sinon": "^14.0.1"
  }
}
